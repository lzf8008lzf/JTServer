<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%highlight{%d{yyyy-MM-dd HH:mm:ss.SSS} [%t]} %-5level %logger{36}%X{context} - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>

        <Root level="warn">
            <AppenderRef ref="Console"/>
        </Root>

        <Logger name="com.tuniondata" level="debug" />
        <!--<Logger name="org.springframework.amqp" level="debug" />-->

    </Loggers>
</Configuration>
